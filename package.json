{
  "name": "babel-preset-zero",
  "version": "0.1.0",
  "description": "[wip]: babelrc for zero config",
  "license": "MIT",
  "repository": "akameco/babel-preset-zero",
  "author": "akameco <akameco.t@gmail.com> (akameco.github.io)",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "add-contributor": "all-contributors add",
    "precommit": "yarn flow && lint-staged",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "flow": "flow",
    "lint": "eslint .",
    "test": "jest",
    "test:cov": "jest --coverage --ci --runInBand",
    "test:ci": "yarn flow && yarn lint && yarn test:cov"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "babel",
    "babelrc",
    "babel-preset"
  ],
  "dependencies": {
    "babel-macros": "^1.2.0",
    "babel-plugin-minify-dead-code-elimination": "^0.4.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "ptils": "^0.2.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.11.1",
    "babel-core": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-precure": "^2.6.0",
    "flow-bin": "^0.69.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-in-case": "^1.0.2",
    "lint-staged": "^7.0.4",
    "prettier": "^1.12.0"
  }
}
