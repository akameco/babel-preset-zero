{
  "name": "babel-preset-zero",
  "version": "0.7.0",
  "description": "babelrc for zero config",
  "license": "MIT",
  "repository": "akameco/babel-preset-zero",
  "author": "akameco <akameco.t@gmail.com> (akameco.github.io)",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "add-contributor": "all-contributors add",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "flow": "flow",
    "lint": "eslint .",
    "test": "jest",
    "test:cov": "jest --coverage --ci --runInBand",
    "test:ci": "yarn flow && yarn lint && yarn test:cov"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "babel",
    "babelrc",
    "babel-preset"
  ],
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "7.3.2",
    "@babel/plugin-transform-modules-commonjs": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "babel-plugin-macros": "2.5.0",
    "babel-plugin-minify-dead-code-elimination": "0.5.0",
    "ptils": "0.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.3.3",
    "all-contributors-cli": "6.1.1",
    "eslint": "5.14.0",
    "eslint-config-precure": "4.7.0",
    "flow-bin": "0.93.0",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "jest-in-case": "1.0.2",
    "lint-staged": "8.1.4",
    "prettier": "1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn flow && lint-staged"
    }
  }
}
